{
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "c:\\nginx\\nginx-1.17.6\\logs\\access.log",
						"log_group_name": "IAPS",
						"log_stream_name": "nginx-access-log"
					},
					{
						"file_path": "c:\\nginx\\nginx-1.17.6\\logs\\error.log",
						"log_group_name": "IAPS",
						"log_stream_name": "nginx-error-log"
					},
					{
						"file_path": "C:\\Program Files (x86)\\I2N\\IapsNDeliusInterface\\Log\\XMLTRANSFER.LOG",
						"log_group_name": "IAPS",
						"log_stream_name": "i2n-xmltransfer.log"
					},
					{
						"file_path": "C:\\Program Files (x86)\\I2N\\IapsNDeliusInterface\\Log\\DAILY\\DAYSUMMARY.LOG",
						"log_group_name": "IAPS",
						"log_stream_name": "i2n-daysummary.log"
					},
					{
						"file_path": "C:\\Program Files (x86)\\I2N\\IapsIMInterface\\Log\\IMIAPSIF.LOG",
						"log_group_name": "IAPS",
						"log_stream_name": "i2n-iapsif.log"
					},
					{
						"file_path": "C:\\Setup\\Backup-IAPS-Configs.log",
						"log_group_name": "IAPS",
						"log_stream_name": "Backup-IAPS-Configs.log"
					}  
				]
			},
			"windows_events": {
				"collect_list": [
					{
						"event_format": "xml",
						"event_levels": [
							"WARNING",
							"ERROR",
							"CRITICAL"
						],
						"event_name": "System",
						"log_group_name": "IAPS",
						"log_stream_name": "system-events"
					},
					{
						"event_format": "xml",
						"event_levels": [
							"WARNING",
							"ERROR",
							"CRITICAL"
						],
						"event_name": "Application",
						"log_group_name": "IAPS",
						"log_stream_name": "application-events"
					}
				]
			}
		}
	},
	"metrics": {
		"append_dimensions": {
			"AutoScalingGroupName": "${aws:AutoScalingGroupName}"
		},
		"metrics_collected": {
			"LogicalDisk": {
				"measurement": [
					"% Free Space"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"Memory": {
				"measurement": [
					"% Committed Bytes In Use"
				],
				"metrics_collection_interval": 60
			},
			"Paging File": {
				"measurement": [
					"% Usage"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"PhysicalDisk": {
				"measurement": [
					"% Disk Time"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"*"
				]
			},
			"Processor": {
				"measurement": [
					"% User Time",
					"% Idle Time",
					"% Interrupt Time"
				],
				"metrics_collection_interval": 60,
				"resources": [
					"_Total"
				]
			},
			"statsd": {
				"metrics_aggregation_interval": 60,
				"metrics_collection_interval": 30,
				"service_address": ":8125"
			}
		}
	}
}
